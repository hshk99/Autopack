# RESEARCH_API_ENABLED=false

# =============================================================================
# NETWORKING & CALLBACKS
# =============================================================================

# ngrok URL (for tunneling local dev to internet)
# Optional for interactive approval buttons in notifications
# Get from: https://ngrok.com/
# Format: https://your-ngrok-subdomain.ngrok.io
# Required: No
# Default: None
# NGROK_URL=https://your-ngrok-url.ngrok.io

# Backend callback URL
# URL where external services should send callbacks
# Used for approval webhooks, async notifications
# Local dev: http://localhost:8001
# Production: https://api.example.com
# Required: No
# Default: http://localhost:8001
# AUTOPACK_CALLBACK_URL=http://localhost:8001

# Autopack API URL
# Base URL for calling other Autopack API instances
# Used for inter-node communication in distributed setup
# Required: No
# Default: http://localhost:8001
# AUTOPACK_API_URL=http://localhost:8001

# API startup timeout (seconds)
# How long to wait for API to be ready before starting workers
# Required: No
# Default: 60
# AUTOPACK_API_STARTUP_TIMEOUT_SECONDS=60

# =============================================================================
# DEPLOYMENT & INFRASTRUCTURE
# =============================================================================

# Port for API server
# HTTP listen port
# Examples: 8000 (FastAPI default), 8001, 5000
# Required: No
# Default: 8000
PORT=8000

# Public read access
# Allow unauthenticated public read access to certain endpoints
# Recommended: false in production
# Required: No
# Default: false
# AUTOPACK_PUBLIC_READ=false

# Target repository path
# Directory containing the git repository to work with
# Docker: typically /workspace (mounted volume)
# Local: relative path or absolute path
# Required: No
# Default: current directory
# TARGET_REPO_PATH=/workspace

# CI Profile: normal | strict
# normal: standard CI checks
# strict: additional validation and stricter rules
# Used by preflight gate to enforce standards
# Required: No
# Default: normal
# CI_PROFILE=normal

# Skip CI checks
# Set to true to skip certain CI validation steps (dev only)
# Not recommended for production or production-like environments
# Required: No
# Default: false
# AUTOPACK_SKIP_CI=false

# Skip run existence check
# Don't verify run exists before allowing certain operations
# Useful for testing but dangerous in production
# Required: No
# Default: false
# AUTOPACK_SKIP_RUN_EXISTENCE_CHECK=false

# Skip CI enforcement for BUILD113 code (temporary workaround)
# Allows skipping specific BUILD113 CI failures
# ONLY for temporary testing, must be removed before merge
# Required: No
# Default: false
# AUTO_APPROVE_BUILD113=false

# Autopack version
# Version string for tracking deployed version
# Should match git tag or package version
# Required: No
# Default: None
# AUTOPACK_VERSION=1.0.0

# Auto-fix TypeScript issues
# Automatically fix fixable TypeScript/ESLint issues
# Required: No
# Default: false
# AUTOPACK_AUTOFIX_TS=false

# Enable OAuth plaintext persistence (DEV ONLY)
# Allows persisting OAuth tokens without encryption (INSECURE)
# NEVER use in production
# Required: No
# Default: false
# AUTOPACK_OAUTH_ALLOW_PLAINTEXT_PERSISTENCE=false

# =============================================================================
# EXTERNAL TOOLS & UTILITIES
# =============================================================================

# WizTree executable path
# Path to WizTree.exe (Windows disk space analyzer)
# Used for analyzing file system and storage
# Required: No
# Default: None
# WIZTREE_PATH=C:\Program Files\WizTree\WizTree.exe

# Enable live trading features
# Enables trading-related APIs and operations
# Requires appropriate licenses and configuration
# Required: No
# Default: false
# LIVE_TRADING_ENABLED=false

# =============================================================================
# INTERNAL / DEBUGGING
# =============================================================================

# Python path (internal use)
# Rarely needs to be set manually - managed by Python runtime
# Required: No
# Default: Auto-determined
# PYTHONPATH=/workspace:/workspace/src

# Python UTF-8 mode (internal use)
# Ensures UTF-8 encoding in Python
# Required: No
# Default: Auto-set to 1
# PYTHONUTF8=1

# Local application data directory
# Windows-specific: AppData\Local directory
# Required: No
# Default: Windows %LOCALAPPDATA%
# LOCALAPPDATA=C:\Users\username\AppData\Local

# Debug database identity
# Enable detailed logging of database identity/connection info
# Required: No
# Default: false
# DEBUG_DB_IDENTITY=false

# Testing mode flag
# Indicates test environment - some features may be disabled/mocked
# Required: No
# Default: None
# TESTING=true

# Pytest current test (set by pytest automatically)
# Contains name of currently running test
# Auto-set by pytest, do not set manually
# Required: No
# Default: None
# PYTEST_CURRENT_TEST=test_module::test_function

# =============================================================================
# QUICK START EXAMPLES
# =============================================================================

# MINIMAL DEVELOPMENT SETUP:
# Uncomment and set only these for local development:
#   AUTOPACK_ENV=development
#   DATABASE_URL=postgresql://autopack:autopack@localhost:5432/autopack
#   REDIS_URL=redis://localhost:6379/0
#   QDRANT_URL=http://localhost:6333
#   ANTHROPIC_API_KEY=your-key-here
#   PORT=8000
#   LOG_LEVEL=DEBUG

# PRODUCTION SETUP:
# Ensure these are set for production deployment:
#   AUTOPACK_ENV=production
#   DATABASE_URL (use managed Postgres + DATABASE_URL_FILE)
#   REDIS_URL (use managed Redis)
#   QDRANT_URL (use Qdrant Cloud or self-managed instance)
#   ANTHROPIC_API_KEY (use AUTOPACK_API_KEY_FILE)
#   AUTOPACK_API_KEY (use AUTOPACK_API_KEY_FILE)
#   JWT_PRIVATE_KEY (use JWT_PRIVATE_KEY_FILE)
#   JWT_PUBLIC_KEY (use JWT_PUBLIC_KEY_FILE)
#   ARTIFACT_REDACTION_ENABLED=true
#   ARTIFACT_READ_SIZE_CAP_BYTES=1048576
